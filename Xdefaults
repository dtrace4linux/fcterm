! Key bindings to emulate a VT-100. Although fcterm emulates them anyway
! with the default bindings, the issue of key bindings on a non-VT100
! keyboard is a problem since each software package assumes different
! key layouts. The following key descriptions are enough for a certain
! package I use. Your mileage may vary.
vt100*sunFunctionKeys:	false
vt100*VT100.Translations:	#override \
	<Key>F1:	string("OP")\n\
	<Key>F2:	string("OQ")\n\
	<Key>F3:	string("OR")\n\
	<Key>F4:	string("OS")\n\
	<Key>F5:	string("OT")\n\
	<Key>F6:	string("OU")\n\
	<Key>F7:	string("OV")\n\
	<Key>F8:	string("OW")\n\
	<Key>F9:	string("OX")\n\
	<Key>F10:	string("OY")\n\
	<Key>F11:	string("OZ")\n\
	<Key>F12:	string("O[")\n\
	<Key>KP_0:	string("Op")\n\
	<Key>KP_1:	string("Oq")\n\
	Shift<Key>KP_2:	string("Or")\n\
	<Key>KP_3:	string("Os")\n\
	Shift<Key>KP_4:	string("Ot")\n\
	<Key>KP_5:	string("Ou")\n\
	Shift<Key>KP_6:	string("Ov")\n\
	<Key>KP_7:	string("Ow")\n\
	Shift<Key>KP_8:	string("Ox")\n\
	<Key>KP_9:	string("Oy")\n\
	<Key>KP_Multiply:	string("Ol")\n\
	<Key>KP_Subtract:	string("Om")\n\
	<Key>KP_Add:	string("On")\n

!	The following entry should be used for running fcterm in a mode
!	which is compatable with CRISP. (Set CRTERM to xcrisp or xcrisp-col).
