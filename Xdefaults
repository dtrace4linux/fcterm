! Key bindings to emulate an Interactive Unix console 
*iscterm*VT100.Translations:	#override \
	Shift<Key>F1:	string("Op")\n\
	Shift<Key>F2:	string("Oq")\n\
	Shift<Key>F3:	string("Or")\n\
	Shift<Key>F4:	string("Os")\n\
	Shift<Key>F5:	string("Ot")\n\
	Shift<Key>F6:	string("Ou")\n\
	Shift<Key>F7:	string("Ov")\n\
	Shift<Key>F8:	string("Ow")\n\
	Shift<Key>F9:	string("Ox")\n\
	Shift<Key>F10:	string("Oy")\n\
	Shift<Key>F11:	string("Oz")\n\
	Shift<Key>F12:	string("O{")\n\
	Ctrl<Key>F1:	string("O")\n\
	Ctrl<Key>F2:	string("O")\n\
	Ctrl<Key>F3:	string("O")\n\
	Ctrl<Key>F4:	string("O")\n\
	Ctrl<Key>F5:	string("O")\n\
	Ctrl<Key>F6:	string("O")\n\
	Ctrl<Key>F7:	string("O")\n\
	Ctrl<Key>F8:	string("O")\n\
